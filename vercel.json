{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron/process-queue",
      "schedule": "*/1 * * * *"
    }
  ],
"builds": [
    {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist"
        }
    },
    {
        "src": "/backend/src/index.ts",
        "use": "@vercel/node"
    }
],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/index.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}